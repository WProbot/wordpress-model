  "require": {
    "php": ">=7.1",
    "widoz/bem": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.5.5",
    "brain/monkey": "dev-master",
    "inpsyde/php-coding-standards": "^0.13.3"
  }
