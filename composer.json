{
  "name": "widoz/wordpress-model",
  "type": "wordpress-plugin",
  "description": "Reusable models for your themes or plugins",
  "keywords": [
    "wordpress",
    "plugin",
    "themes",
    "template",
    "mvc",
    "psr"
  ],
  "homepage": "https://github.com/widoz/wordpress-theme-model",
  "license": "GPL-2.0-only",
  "authors": [
    {
      "name": "Guido Scialfa",
      "email": "dev@guidoscialfa.com",
      "homepage": "http://www.guidoscialfa.com",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/widoz/wordpress-theme-model/issues"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "WordPressModel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WordPressModel\\": "src",
      "WordPressModel\\Tests\\": "tests"
    }
  },
  "require": {
    "php": ">=7.1",
    "widoz/bem": "dev-develop"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "brain/monkey": "dev-master",
    "inpsyde/php-coding-standards": "^0.13.3",
    "widoz/project-tests-helper": "dev-master"
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.1.x-dev"
    }
  }
}
